{
  "name": "fp-ts-routing",
  "version": "0.5.4",
  "description": "A type-safe routing library for TypeScript",
  "files": [
    "lib",
    "es6"
  ],
  "main": "lib/index.js",
  "module": "es6/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "engines": {
    "npm": ">= 6.14"
  },
  "scripts": {
    "prepare": "husky install",
    "clean": "rm -rf lib/* es6/*",
    "prettier:fix": "prettier --write \"{src,test}/**/*.ts\"",
    "check": "tsc",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "dtslint": "ts-node ./scripts/dtslint.ts",
    "jest": "jest --ci",
    "pretest": "npm run check && npm run lint && npm run dtslint",
    "test": "npm run jest",
    "posttest": "npm run docs",
    "mocha": "mocha -r ts-node/register test/*.ts",
    "prebuild": "npm run clean",
    "build": "tsc -p ./tsconfig.build.json && tsc -p ./tsconfig.build-es6.json",
    "postbuild": "import-path-rewrite",
    "prepublishOnly": "npm run build",
    "docs": "docs-ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gcanti/fp-ts-routing.git"
  },
  "author": "Giulio Canti <giulio.canti@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gcanti/fp-ts-routing/issues"
  },
  "homepage": "https://github.com/gcanti/fp-ts-routing",
  "peerDependencies": {
    "fp-ts": "^2.0.1",
    "io-ts": "^2.0.0"
  },
  "devDependencies": {
    "@definitelytyped/dtslint": "^0.0.163",
    "@types/jest": "^29.5.2",
    "@types/node": "^16.18.36",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "docs-ts": "^0.6.7",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "fp-ts": "^2.0.1",
    "husky": "^8.0.3",
    "import-path-rewrite": "github:gcanti/import-path-rewrite",
    "io-ts": "^2.0.0",
    "jest": "^29.5.0",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "tags": [
    "typescript",
    "functional-programming",
    "routing",
    "applicative"
  ],
  "keywords": [
    "typescript",
    "functional-programming",
    "routing",
    "applicative"
  ]
}
